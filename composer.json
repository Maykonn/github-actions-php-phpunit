{
  "name": "maykonn/github-actions-php-phpunit",
  "description": "Github Actions for PHP with PHPUnit Tests",
  "autoload": {
    "classmap": [
      "src/"
    ]
  },
  "require-dev": {
    "phpunit/phpunit": "^8"
  },
  "scripts": {
    "test": "./vendor/bin/phpunit --bootstrap vendor/autoload.php tests/",
    "testdox": "./vendor/bin/phpunit --bootstrap vendor/autoload.php --testdox --colors=always tests/"
  },
  "license": "MIT"
}